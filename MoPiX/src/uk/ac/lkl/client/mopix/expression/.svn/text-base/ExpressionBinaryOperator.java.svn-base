package uk.ac.lkl.client.mopix.expression;

import com.google.gwt.xml.client.Node;
import uk.ac.lkl.client.mopix.MoPiXObject;

public class ExpressionBinaryOperator extends Expression {
    protected Expression arg1;
    protected Expression arg2;

    public ExpressionBinaryOperator(Node xml, MoPiXObject object,
	    Expression arg1, Expression arg2) {
	super();
	setXML(xml, object);
	this.arg1 = arg1;
	this.arg2 = arg2;
    }
}
